class Kangaroo extends Animal{
    String name;

    Kangaroo(String name, int hungry, int [] location){
        super(hungry, location);
        this.name = name;
    }

    public String toString(){
        return("I am a kangaroo. My name is "+this.name+". I am "+super.toString());
    }


    void eat(){
        System.out.println("I'm vegan and I love eating grass.. ");
    }

    void sleep(){
        //when the animal considered asleep
        System.out.println("I go to bed after the sunset");
    }

    void makeNoise(){
        //sound of noise
        System.out.println("I'm a kangaroo doing noise");
    }
}