class Person{

    // attributes
    private String givenName;
    private String lastName;
    private int age;
    private String gender;
    private String citizenship;

    // basic constructor
    public Person(){
        this.givenName = "unknown";
        this.lastName = "unknown";
        this.age = 0;
    }

    // parametric constructor
    public Person(String givenName, String lastName, int age, String gender, String citizenship){
        this.givenName = givenName;
        this.lastName = lastName;
        this.age = age;
        this.gender = gender;
        this.citizenship = citizenship;
    }

    // accessors
    public String get_givenName(){
        return this.givenName;
    }
    public String get_lastName(){
        return this.lastName;
    }
    public int get_age(){
        return this.age;
    }
    public String get_gender(){
        return this.gender;
    }
    public String get_citizenship(){
        return this.citizenship;
    }

    // mutators or modifiers
    public void set_givenName(String givenName){
        this.givenName = givenName;
    }
    public void set_lastName(String lastName){
        this.lastName = lastName;
    }
    public void set_age(int age){
        this.age = age;
    }
    public void set_gender(String gender){
        this.gender = gender;
    }
    public void set_citizenship(String citizenship){
        this.citizenship = citizenship;
    }
}