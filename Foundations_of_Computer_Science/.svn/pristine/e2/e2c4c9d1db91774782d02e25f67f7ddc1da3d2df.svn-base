class Animal{
    private String name;
    private int age;
    private String favorite_food;

    public Animal(){

    }

    public Animal(String name, int age, String favorite_food){
        this.name = name;
        this.age = age;
        this.favorite_food = favorite_food;
    }

    public void print_information(){
        System.out.println(" Animal name : "+this.name+"\n Animal age: "+this.age+"\n Animal's favorite food: "+this.favorite_food);
    }

    public String get_name(){
        return this.name;
    }
    public int get_age(){
        return this.age;
    }
    public String get_favorite_food(){
        return this.favorite_food;
    }
    public void set_name(String name){
        this.name = name;
    }
    public void set_age(int age){
        this.age = age;
    }
    public void set_favorite_food(String favorite_food){
        this.favorite_food = favorite_food;
    }
}