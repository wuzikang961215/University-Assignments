public class Node{

    // attributes
    private int data;
    private Node left;
    private Node right;

    // constructor
    public Node(int data){
        this.data = data;
        this.left = null;
        this.right = null;
    }

    // accessors
    public int getData(){
        return this.data;
    }
    public Node getLeft(){
        return this.left;
    }
    public Node getRight(){
        return this.right;
    }

    // mutators
    public void setData(int data){
        this.data = data;
    }
    public void setLeft(Node left){
        this.left = left;
    }
    public void setRight(Node right){
        this.right = right;
    }

    // print the data saved in the node
    public void printNode(){
        System.out.println("Node data: <" + data + ">");
    }
}