public abstract class VehicleAbstract implements Vehicle{
    // attributes
    String color;
    int year;
    double numberGears;

    // basic constructor
    VehicleAbstract(){
        
    }

    // parametric constructor
    public VehicleAbstract(String color, int year, double numberGears){
        this.color = color;
        this.year = year;
        this.numberGears = numberGears;
    }

    // abstract method
    public abstract void abstractMethod();
}